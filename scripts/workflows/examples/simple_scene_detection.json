{
  "version": "1.0",
  "metadata": {
    "name": "Simple Scene Detection",
    "description": "Detect scenes in a video and upload to S3"
  },
  "nodes": [
    {
      "id": "load_video",
      "type": "LoadVideoNode",
      "position": {"x": 100, "y": 100},
      "inputs": {
        "video_path": "/path/to/video.mov"
      }
    },
    {
      "id": "detect_scenes",
      "type": "DetectScenesNode",
      "position": {"x": 300, "y": 100},
      "inputs": {
        "threshold": 22.0,
        "min_frames": 8
      }
    },
    {
      "id": "upload_s3",
      "type": "UploadS3Node",
      "position": {"x": 500, "y": 100},
      "inputs": {
        "s3_destination": "s3://bucket-name/path/",
        "parallel": 8
      }
    }
  ],
  "connections": [
    {
      "from": {"node": "load_video", "output": "video_path"},
      "to": {"node": "detect_scenes", "input": "video_path"}
    },
    {
      "from": {"node": "detect_scenes", "output": "output_directory"},
      "to": {"node": "upload_s3", "input": "local_directory"}
    }
  ]
}

